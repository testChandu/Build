contract simpleStorage3 {

//this will get initialized to 0!
    uint256  favouriteNumber;
    bool favouriteBool;
      struck People {
      unit256 favoriteNumber;
      string name
    }
    People [] public people;
    mapping(string=>uint256) public nameToFavoriteNumber;
    function store(unit256_favoriteNumber)public{
    favoriteNumber=_favoriteNumber;
    }
    function retrieve () public view returns(uint256){
    return favoriteNumber;
    }

function addPerson( string memory _name,unit256_favoriteNumber)public{
people.push(People(_favorateNumber,_name));
nameToFavariteNumber[]=_favoriteNumber;
}
}
